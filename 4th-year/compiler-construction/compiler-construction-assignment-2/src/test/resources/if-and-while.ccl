integer add(x:integer, y:integer)
{
    if(x < 10 && y < 0) {
        x = x + 1;
        y = y + 1;
    } else {
        y = y - 1;
        x = x - 1;
    }
    while(x < 10) {
        x = x + 1;
    }
    return (x);
}

main
{
    var x:integer;
    var y:integer;
    x = 1;
    y = 2;
    add(x, y);
}
